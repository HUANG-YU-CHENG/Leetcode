#include <stdio.h>

/**
 * Note: The returned array must be malloced, assume caller calls free().
 */
int* twoSum(int* nums, int numsSize, int target, int* returnSize){
int i,j;
int n = numsSize;
int *answer = NULL;
for (i= 0 ; i = n ; i++){ /**/
    for(j = i + 1; j < n; j++) {
        if(target == nums[i]+nums[j]){
        answer[0]=i;
        answer[1]=j;
        return answer;
            }
        }
    }
    return 0;
}
